generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int    @id @default(autoincrement())
  role  Role   @default(USER)
  email String @default("") @unique
  hash  String @default("")
}

enum Role {
  USER
  ADMIN
}